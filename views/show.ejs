<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <script src="https://cdn.tailwindcss.com"></script>
</head>
<body>    
    <div class="w-full min-h-screen bg-zinc-800 px-10 py-8 text-white">
        <div class="cards flex flex-wrap gap-10">
            <% user.books.forEach(function(elem){ %>
            <div class="card w-32">
                <div class=" w-32 h-40 bg-black-200 rounded-lg overflow-hidden">
                    <img src="/images/uploads/<%= elem.image %>" alt="">    
                </div>
               </div>
            <% }) %>
        </div>
    </div>

</body>
</html>